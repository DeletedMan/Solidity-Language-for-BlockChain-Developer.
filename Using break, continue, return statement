pragma solidity ^0.4.25;

contract loop {
    function soNNumbers(uint n) public pure returns(uint)
    {
        uint sum=0;
        for(uint i=0;i<=n;i++)
        {
            sum+=i;
            if(i==5)
            {
                break;
            }
            else
            {
                continue;
            }
        }
        return sum;
    }
}
